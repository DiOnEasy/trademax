<script>
    $(window).on("scroll", function() {
        if($(window).scrollTop() > 50) {
            $(".tmgm-header").addClass("on-scroll");
            if($(window).width() >= 1440) {
                $(".tmgm-main-menu").css('display', 'none');
                $(".tmgm-main-menu-on-scroll").css('display', 'flex');
                $(".tmgm-header-container").css('max-width', '1500px');
                $(".hide-on-scroll").css('display', 'none')
            }
        } else {
            //remove the background property so it comes transparent again (defined in your css)
            $(".tmgm-header").removeClass("on-scroll");
            if($(window).width() >= 1440) {
                $(".tmgm-main-menu").css('display', '');
                $(".tmgm-header").removeClass("on-scroll");
                $(".tmgm-main-menu-on-scroll").css('display', 'none');
                $(".tmgm-header-container").css('max-width', '1224px');
                $(".hide-on-scroll").css('display', 'flex')
            }
        }
    });
    $(window).resize(function() {
        var width = $(window).width();
        if($('.tmgm-header').hasClass('on-scroll') && width < 1024) {
            $(".tmgm-main-menu-on-scroll").css('display', 'none');
            $(".tmgm-header-container").css('max-width', '1224px');
        } else if($('.tmgm-header').hasClass('on-scroll') && width > 1024) {
            $(".tmgm-main-menu-on-scroll").css('display', 'flex');
            $(".hide-on-scroll").css('display', 'none')
            $(".tmgm-main-menu").css('display', 'none');
            // $(".tmgm-header-container").css('max-width', '1500px');
        }
    })

    $('.tmgm-new-menu-wrapper ul li').hover(function() {
        $('.has-submenu').removeClass("active");
        $('.tmgm-submenu-area').removeClass("active");
    })
    $('.tmgm-main-menu-btn').hover(function(){
        // $(this).trigger('click');
        $('.tmgm-submenu-area').removeClass("active");
        $('.has-submenu').removeClass("active");
    });
    $('.has-submenu').click(function() {
        $('.tmgm-submenu-area').addClass("active");
        $('.tmgm-submenu').removeClass("active");
        menuIndex = $(this).index();

        if(!$(this).parent().parent().parent().find('.tmgm-submenu-area').children().eq(menuIndex).hasClass("active")) {
            $(this).parent().parent().parent().find('.tmgm-submenu-area').children().eq(menuIndex).addClass("active");
        }

        $('.has-submenu').removeClass("active");
        // $('.has-submenu').find('.tmgm-submenu').removeClass("active")
        if(!$(this).hasClass("active")) $(this).addClass("active");
        else $(this).removeClass("active");
        if(!$(this).find('.tmgm-submenu').hasClass("active")) $(this).find('.tmgm-submenu').addClass("active");
        else $(this).find('.tmgm-submenu').removeClass("active");
    })
    $('.has-submenu').hover(function () {
        console.log('hover');
        $('.tmgm-submenu-area').addClass("active");
        $('.tmgm-submenu').removeClass("active");
        menuIndex = $(this).index();

        if(!$(this).parent().parent().parent().find('.tmgm-submenu-area').children().eq(menuIndex).hasClass("active")) {
            $(this).parent().parent().parent().find('.tmgm-submenu-area').children().eq(menuIndex).addClass("active");
        }

        $('.has-submenu').removeClass("active");
        // $('.has-submenu').find('.tmgm-submenu').removeClass("active")
        if(!$(this).hasClass("active")) $(this).addClass("active");
        else $(this).removeClass("active");
        if(!$(this).find('.tmgm-submenu').hasClass("active")) $(this).find('.tmgm-submenu').addClass("active");
        else $(this).find('.tmgm-submenu').removeClass("active");
    })
    $('.dropdown.menu').hover(function(){
