<script>
    $(window).ready(function() {
        window.getRootDomain = function () {
            var domain = window.location.hostname;
            var domainArray = domain.split('.');
            domainArray.shift();
            return (domainArray.join('.') == '0.0.1') ? '127.0.0.1' : domainArray.join('.');
        };
        /* Get URL parameter by key */
        const getUrlParameter = function (key) {
            const queryString = window.location.search;
            const urlParams = new URLSearchParams(queryString);

            return urlParams.get(key) ? urlParams.get(key) : "";
        };

        const isEncoded = function (parameter) {
            const base64regex =
                /^([0-9a-zA-Z+/]{4})*(([0-9a-zA-Z+/]{2}==)|([0-9a-zA-Z+/]{3}=))?$/;

            return base64regex.test(parameter);
        };

        /* Get referral code from URL */
        const campaignCode = getUrlParameter("campaign_code");
        const expiryDate = getUrlParameter("expiry_date");

        // console.log(`Root domain: ${getRootDomain()} with Campaign Code: ${campaignCode}`)

        if (campaignCode !== '') {
            // console.log('Processing...')

            window.CAMPAIGN_CODE = isEncoded(campaignCode)
                ? atob(campaignCode)
                : campaignCode;
            window.EXPIRE_DATE = isEncoded(expiryDate)
                ? parseInt(atob(expiryDate))
                : parseInt(expiryDate);

            cookieString = `campaign_code=${CAMPAIGN_CODE}; expires=${EXPIRE_DATE ? EXPIRE_DATE : 30}; path=/; domain=${getRootDomain()};`

            // console.log(`COOKIE : ${cookieString}`)
            // Set cookie
            document.cookie = cookieString

        }
        // else if ($.cookie('campaign_code') && $.cookie('campaign_code') !== '') {
        //     window.CAMPAIGN_CODE = $.cookie('campaign_code')
        // }
        else {
            window.CAMPAIGN_CODE = ''
        }

    })

