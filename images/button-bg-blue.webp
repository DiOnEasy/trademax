        function countryCallback() {
            $("#select-state").select2({
                dropdownParent: $('#registerModal'),
                data: countrySelect2Data,
            });

            $("#country-dial-code").select2({
                dropdownParent: $('#registerModal'),
                data: countryDialCodeSelect2Data,
                escapeMarkup: function(markup) {
                    return markup;
                },
                templateResult: function(data) {
                    return data.html;
                },
                templateSelection: function(data) {
                    return data.text;
                },
                dropdownCssClass: 'pn-select2-dropdown',
            });


            // DEFAULT COUNTRY
            const defaultCountry = findObjectByPropertyValue(countrySelect2Data, 'code', countryCode);
            let defaultCountryName = "Afghanistan"
            let defaultPhoneCo = "+93"

            if(defaultCountry) {
                defaultCountryName = defaultCountry['id'];
                defaultPhoneCo = defaultCountry['phoneCo'];
            }
            $('#select-state').val(defaultCountryName).trigger('change');
            $('#country-dial-code').val(defaultPhoneCo).trigger('change');


            // fix select2 bootstrap modal scroll bug - start
            $('#select-state').each(function () {
                $(this).select2({
                    dropdownParent: $(this).parent(),// fix select2 search input focus bug
                    data: countrySelect2Data
                })
            });
            $(document).on('select2:close', '#select-state', function (e) {
                var evt = "scroll.select2"
                $(e.target).parents().off(evt)
                $(window).off(evt)
            });
            $('#country-dial-code').each(function () {
                $(this).select2({
                    dropdownParent: $(this).parent(),// fix select2 search input focus bug
